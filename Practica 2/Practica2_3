const personas = [
    { nombre: "Ana", edad: 22 },
    { nombre: "Luis", edad: 35 },
    { nombre: "Maria", edad: 28 }
];

const luiss = personas.find(persona => persona.nombre === "Luis");
console.log(luiss);

const totalEdad = personas.reduce((acc, p) => acc + p.edad, 0);
console.log(totalEdad);

personas.forEach(persona => {console.log(persona);});
